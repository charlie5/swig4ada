
echo
echo removing prior build

rm *.ads
rm -f ./tester
rm *.o
rm *.ali
rm *.a


echo
echo generating bindings

../../../gnat/swig_gnat  -gnat -c++  example.i




echo
echo pretty printing ...

gnatpp  -rnb  -I../../..      *.ads    -cargs -gnatX




echo
echo compiling

gprmake  -P   test_example.gpr 






echo
echo running tester ...

./tester
