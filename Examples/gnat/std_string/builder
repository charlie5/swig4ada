
echo
echo removing prior build

rm -f ./tester
rm *.ads
rm *.adb
rm example_wrap.c
rm example_wrap.cxx
rm *.o
rm *.ali
rm *.a


echo
echo generating bindings

../../../gnat/swig_gnat  -gnat -c++  example.i




echo
echo pretty printing ...

gnatpp  -rnb  -I../../..      *.ads    -cargs -gnatX




echo
echo compiling

gprbuild  -P   test_example.gpr 






echo
echo running tester ...

./tester
